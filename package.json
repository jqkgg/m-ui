{
  "name": "@jqkgg/m-ui",
  "version": "1.0.7",
  "description": "一个基于 Vue3 的组件库",
  "type": "module",
  "main": "./dist/m-ui.umd.cjs",
  "module": "./dist/m-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/m-ui.js",
      "require": "./dist/m-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && npm run build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --outDir dist",
    "build:docs": "vitepress build docs",
    "dev:docs": "vitepress dev docs",
    "preview:docs": "vitepress preview docs",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "publish": "npm run build && npm publish --access public"
  },
  "keywords": [
    "vue3",
    "component-library",
    "ui",
    "typescript"
  ],
  "author": "jqkgg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jqkgg/m-ui.git"
  },
  "homepage": "https://github.com/jqkgg/m-ui#readme",
  "bugs": {
    "url": "https://github.com/jqkgg/m-ui/issues"
  },
  "peerDependencies": {
    "echarts": "^5.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vueuse/core": "^10.7.0",
    "autoprefixer": "^10.4.17",
    "echarts": "^5.6.0",
    "echarts-gl": "^2.0.9",
    "element-plus": "^2.13.1",
    "postcss": "^8.4.35",
    "sass": "^1.69.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^5.0.0",
    "vitepress": "^1.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^3.2.2"
  }
}
