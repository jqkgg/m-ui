{
  "name": "m-ui",
  "version": "1.0.0",
  "description": "一个基于 Vue3 的组件库",
  "type": "module",
  "main": "./dist/m-ui.umd.cjs",
  "module": "./dist/m-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/m-ui.js",
      "require": "./dist/m-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && npm run build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --outDir dist",
    "build:docs": "vitepress build docs",
    "dev:docs": "vitepress dev docs",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue3",
    "component-library",
    "ui",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.27",
    "vitepress": "^1.0.0",
    "@vueuse/core": "^10.7.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.17",
    "sass": "^1.69.5"
  },
  "dependencies": {}
}

